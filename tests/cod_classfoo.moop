class Bar {
    int a;

    Bar Bar() {
        this.a = 10;
    }

    int getA(){
        return this.a;
    }

}

class Foo {
    int x;
    
    Foo Foo(int x) { 
        Bar temp;
        int i;
        temp = new Bar();  
        this.x = x;
        for (i = 0; i < 1; i = i + 1) {
            this.x = temp.getA();
        }
    }

    int getX() {
        return this.x;
    }

    void setX(int x) {
        this.x = x;
    }

}

class Main { 
    int main () { 
        Foo test;
        test = new Foo(45);
        printi (test.getX());
        test.setX(30);
        printi (test.getX());
        return 0;
    }
}

