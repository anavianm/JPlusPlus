class Foo {
    int x;
    
    Foo Foo(int x) {   
        this.x = x;
    }

    int getX() {
        return this.x;
    }

    void setX(int x) {
        this.x = x;
    }

}

class Main { 
    int main () { 
        Foo test;
        test = new Foo(45);
        printi (test.getX());
        test.setX(30);
        printi (test.getX());
        return 0;
    }
}

